load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "day25",
    srcs = ["day25.go"],
    importpath = "github.com/Saser/pdp/adventofcode/go/year2020/day25",
    visibility = ["//adventofcode/go:__subpackages__"],
