go_test(
    name = "day25_test",
    srcs = ["day25_test.go"],
    data = glob(["testdata/**"]) + ["//adventofcode/inputs:2020/25"],
    embed = [":day25"],
    deps = ["//adventofcode/go/testcase"],
)
